{
  "name": "handyman-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "npm run prod",
    "dev": "cross-env NODE_ENV=development DEBUG=mylaw-conference-api:server npm run watch ./src/bin/www",
    "prod": "npm run build && cross-env NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.0",
    "rimraf": "^3.0.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4"
  }
}
